{
  "name": "figma-turkish-data",
  "version": "1.1.0",
  "description": "Turkish data supplier plugin, provides names, locations, business titles and dates in various formats.",
  "author": "Ozgur Gunes",
  "authorEmail": "o.gunes@gmail.com",
  "homepage": "https://github.com/ozgurgunes/Figma-Turkish-Data",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ozgurgunes/Figma-Turkish-Data.git"
  },
  "bugs": {
    "url": "https://github.com/ozgurgunes/Figma-Turkish-Data/issues"
  },
  "scripts": {
    "build": "build-figma-plugin --typecheck --minify",
    "watch": "build-figma-plugin --typecheck --watch"
  },
  "figma-plugin": {
    "editorType": [
      "figma"
    ],
    "id": "turkish-date",
    "name": "Turkish Data",
    "menu": [
      {
        "name": "Name",
        "main": "src/supplyName/main.ts",
        "ui": "src/supplyName/ui.tsx"
      },
      {
        "name": "Location",
        "main": "src/supplyLocation/main.ts",
        "ui": "src/supplyLocation/ui.tsx"
      },
      {
        "name": "Datetime",
        "main": "src/supplyDatetime/main.ts",
        "ui": "src/supplyDatetime/ui.tsx"
      },
      {
        "name": "Business",
        "main": "src/supplyBusiness/main.ts",
        "ui": "src/supplyBusiness/ui.tsx"
      },
      {
        "name": "Price",
        "main": "src/supplyPrice/main.ts",
        "ui": "src/supplyPrice/ui.tsx"
      },
      {
        "name": "Phone",
        "main": "src/supplyPhone/main.ts",
        "ui": "src/supplyPhone/ui.tsx"
      },
      {
        "name": "Email",
        "main": "src/supplyEmail/main.ts",
        "ui": "src/supplyEmail/ui.tsx"
      },
      {
        "name": "Calendar",
        "main": "src/supplyCalendar/main.ts",
        "ui": "src/supplyCalendar/ui.tsx"
      }
    ],
    "relaunchButtons": {
      "turkishName": {
        "name": "Turkish Names",
        "main": "src/supplyName/main.ts",
        "ui": "src/supplyName/ui.tsx"
      },
      "turkishDateTime": {
        "name": "Turkish Dates",
        "main": "src/supplyDatetime/main.ts",
        "ui": "src/supplyDatetime/ui.tsx"
      },
      "turkishLocation": {
        "name": "Turkish Locations",
        "main": "src/supplyLocation/main.ts",
        "ui": "src/supplyLocation/ui.tsx"
      },
      "turkishBusiness": {
        "name": "Turkish Businesses",
        "main": "src/supplyBusiness/main.ts",
        "ui": "src/supplyBusiness/ui.tsx"
      },
      "turkishPrice": {
        "name": "Turkish Prices",
        "main": "src/supplyPrice/main.ts",
        "ui": "src/supplyPrice/ui.tsx"
      },
      "turkishPhone": {
        "name": "Turkish Phone",
        "main": "src/supplyPhone/main.ts",
        "ui": "src/supplyPhone/ui.tsx"
      },
      "turkishEmail": {
        "name": "Turkish Email",
        "main": "src/supplyEmail/main.ts",
        "ui": "src/supplyEmail/ui.tsx"
      },
      "turkishCalendar": {
        "name": "Turkish Calendar",
        "main": "src/supplyCalendar/main.ts",
        "ui": "src/supplyCalendar/ui.tsx"
      }
    }
  },
  "dependencies": {
    "@create-figma-plugin/ui": "^2.2.0",
    "@create-figma-plugin/utilities": "^2.2.0",
    "latinize": "^0.5.0",
    "preact": "^10.12.0",
    "react-datepicker": "^4.10.0"
  },
  "devDependencies": {
    "@create-figma-plugin/build": "^2.2.0",
    "@create-figma-plugin/tsconfig": "^2.2.0",
    "@figma/plugin-typings": "^1.58.0",
    "@types/latinize": "^0.2.15",
    "@types/react": "^18.0.27",
    "@types/react-datepicker": "^4.8.0",
    "typescript": "^4.9.5"
  }
}
